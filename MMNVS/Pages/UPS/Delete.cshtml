        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.SNMPOIDBatteryTime)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.SNMPOIDLoad)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.SNMPOIDLoad)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.Notes)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.Notes)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="UPS.Id" />
        <input type="submit" value="Smazat" class="btn btn-danger" /> |
        <a asp-page="./Index">Zpět</a>
    </form>
</div>

@page
@model MMNVS.Pages.UPS.DeleteModel

@{
    ViewData["Title"] = "Odstranění UPS";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h2>Odstranění UPS</h2>

<h3>Opravdu že chcete smazat tuto UPS?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.Model)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.Model)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.Producer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.Producer)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.IPAddress)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.IPAddress)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.UPS.SNMPOIDState)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UPS.SNMPOIDState)
